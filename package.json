{
  "name": "react-web-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "deploy": "gcloud run deploy react-web-app --source . --platform managed --allow-unauthenticated --region us-central1",
    "deploy:env": "gcloud run deploy react-web-app --source . --platform managed --allow-unauthenticated --region us-central1 --set-env-vars \"$(awk '!/^#/ && /=/{printf \"%s%s\", (NR==1 ? \"\" : \",\"), $0}' .env)\"",
    "shadcn": "shadcn",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
